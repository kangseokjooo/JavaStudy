package java_class;

//static 없는 경우 
class withoutStatic{
	private int register=0;
	private String name;
	
	public withoutStatic(String name){
		this.name=name;
		register++;
	}
	public int getRegister() {
		return register;
	}
}
//static 변수가 있는 경우
class withStatic{
	
}

public class CompareStatic {

	public static void main(String[] args) {
		
	}

}
