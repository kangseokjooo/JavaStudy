package java_first_project;

public class Except {

	public static void main(String[] args) {
		try {
			int num1=4;
			int num2=0;
			System.out.println(num1+"/"+num2+"="+(num1/num2));
			
		} catch (ArithmeticException e) {
			System.out.println("0으로 나누어서 문제가 발생!");
		}
		System.out.println("11111");
	}

}
