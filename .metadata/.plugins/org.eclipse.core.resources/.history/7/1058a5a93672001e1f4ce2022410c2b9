package java_class;

public class Car {
	//멤버 변수
	private int speed;
	
	public void start() {
		System.out.println("Car Start");
	}

	public int getSpeed() {
		return speed;
	}

	public void setSpeed(int speed) {
		if(speed>0) {
			this.speed = speed;			
		}
	}
	
//	//setter메소드 
